    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start storybook",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "storybook"
            ],
        },
        {
            "name": "Generate icons",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "yarn",
            "runtimeArgs": [
                "run",
                "download:icons"
            ],
        },
        {
            "name": "Download icons",
            "request": "launch",
            "runtimeArgs": [
                "run",
                "download:icons"
            ],
            "runtimeExecutable": "yarn",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
    ]
}