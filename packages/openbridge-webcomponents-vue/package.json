{
  "name": "openbridge-webcomponents-vue",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:declarations && vite build",
    "typecheck": "vue-tsc --noEmit",
    "build:declarations": "vue-tsc --declaration --emitDeclarationOnly && node ./scripts/rename.js",
    "preview": "vite preview"
  },
  "version": "0.0.0",
  "dependencies": {
    "@lit-labs/vue-utils": "^0.1.0",
    "openbridge-webcomponents": "^0.0.0",
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^9.0.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "typescript": "^5.3.3",
    "vite": "^3.1.8",
    "vue-tsc": "^1.8.8"
  },
  "files": [
    "components/alert-button/AlertButton.*",
    "components/alert-menu/AlertMenu.*",
    "components/app-button/AppButton.*",
    "components/app-menu/AppMenu.*",
    "components/breadcrumb/Breadcrumb.*",
    "components/brilliance-menu/BrillianceMenu.*",
    "components/button/Button.*",
    "components/card-list-button/CardListButton.*",
    "components/clock/Clock.*",
    "components/demo/Demo.*",
    "components/divider/Divider.*",
    "components/icon/Icon.*",
    "components/icon-button/IconButton.*",
    "components/icons/Obi14Alerts.*",
    "components/input/Input.*",
    "components/navigation-item/NavigationItem.*",
    "components/navigation-menu/NavigationMenu.*",
    "components/notification-button/NotificationButton.*",
    "components/notification-message/NotificationMessage.*",
    "components/notification-message-item/NotificationMessageItem.*",
    "components/slider/Slider.*",
    "components/toggle-button-group/ToggleButtonGroup.*",
    "components/toggle-button-option/ToggleButtonOption.*",
    "components/toggle-switch/ToggleSwitch.*",
    "components/tooltip/Tooltip.*",
    "components/top-bar/TopBar.*"
  ]
}
