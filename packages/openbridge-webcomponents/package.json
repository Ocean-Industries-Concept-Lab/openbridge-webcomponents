{
  "name": "@oicl/openbridge-webcomponents",
  "version": "0.0.15",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ocean-Industries-Concept-Lab/openbridge-webcomponents.git",
    "directory": "packages/openbridge-webcomponents"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "yarn run build:translations && yarn run typecheck && yarn run build:ts && yarn run analyze && yarn run build:wrappers",
    "build:ts": "vite build",
    "build:ts:watch": "vite build --watch",
    "build:translations": "lit localize build",
    "build:wrappers": "lit labs gen --framework=vue --framework=react --out=.. && node fix-generated.cjs",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "download:icons": "tsx ./script/download-icons.ts",
    "new:component": "tsx ./new-component.ts",
    "test-storybook": "test-storybook",
    "prepack": "yarn build",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\"  --write",
    "format:check": "prettier \"**/*.{cjs,html,js,json,md,ts}\"  --check",
    "lint": "yarn run lint:lit-analyzer && yarn run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/**/*.stories.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/**/*.stories.ts\" --watch",
    "release": "release-it"
  },
  "dependencies": {
    "@lit-labs/observers": "^2.0.4",
    "@lit/localize": "^0.12.1",
    "lit": "^3.2.1"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.10.3",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@lit-labs/cli": "^0.6.5",
    "@lit-labs/cli-localize": "^0.2.1",
    "@lit-labs/gen-wrapper-react": "^0.3.3",
    "@lit-labs/gen-wrapper-vue": "^0.4.0",
    "@open-wc/lit-helpers": "^0.7.0",
    "@playwright/test": "^1.49.0",
    "@storybook/addon-essentials": "^8.4.4",
    "@storybook/addon-interactions": "^8.4.4",
    "@storybook/addon-links": "^8.4.4",
    "@storybook/addon-storysource": "^8.4.4",
    "@storybook/addon-themes": "^8.4.4",
    "@storybook/blocks": "^8.4.4",
    "@storybook/manager-api": "^8.4.4",
    "@storybook/test": "^8.4.4",
    "@storybook/test-runner": "^0.19.1",
    "@storybook/theming": "^8.4.6",
    "@storybook/web-components": "^8.4.4",
    "@storybook/web-components-vite": "^8.4.4",
    "@topcli/prompts": "^2.0.0",
    "@types/jest-image-snapshot": "^6",
    "@types/node": "^22.4.1",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "dotenv": "^16.3.1",
    "eslint": "^9.15.0",
    "eslint-plugin-storybook": "^0.11.1",
    "figma-api": "^1.12.0",
    "globals": "^15.12.0",
    "jest-image-snapshot": "^6.2.0",
    "lit-analyzer": "^2.0.3",
    "nodemon": "^3.0.2",
    "playwright": "^1.49.0",
    "postcss": "^8.4.49",
    "postcss-mixins": "^11.0.3",
    "postcss-nesting": "^13.0.1",
    "prettier": "^3.3.3",
    "release-it": "^17.1.1",
    "rollup-plugin-postcss-lit": "^2.1.0",
    "storybook": "^8.4.4",
    "storybook-addon-tag-badges": "^1.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.2.7",
    "vite-plugin-dts": "^4.3.0"
  },
  "types": "./dist/index.d.ts",
  "customElements": "custom-elements.json"
}
