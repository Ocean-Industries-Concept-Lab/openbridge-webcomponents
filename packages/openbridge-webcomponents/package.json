{
  "name": "openbridge-webcomponents",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "yarn run build:css && yarn run build:ts && yarn run analyze && yarn run build:wrappers",
    "build:watch": "concurrently --kill-others \"yarn run build:ts:watch\" \"yarn run build:css:watch\"",
    "build:ts": "tsc && vite build",
    "build:ts:watch": "vite build --watch",
    "build:css": "tsx build-css.ts",
    "build:css:watch": "nodemon --watch src/components -e css --exec \"yarn run build:css\"",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "download:icons": "tsx ./script/download-icons.ts",
    "test-storybook": "test-storybook",
    "prepack": "yarn build",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\"  --write",
    "format:check": "prettier \"**/*.{cjs,html,js,json,md,ts}\"  --check",
    "lint": "yarn run lint:lit-analyzer && yarn run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/**/*.stories.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --exclude \"src/**/*.stories.ts\" --watch",
    "build:wrappers": "lit labs gen --framework=vue --out=.. && node fix-generated.cjs"
  },
  "dependencies": {
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.9.0",
    "@lit-labs/cli": "^0.6.2",
    "@lit-labs/gen-wrapper-vue": "^0.3.2",
    "@open-wc/lit-helpers": "^0.6.0",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-themes": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/web-components": "^7.6.7",
    "@storybook/web-components-vite": "^7.6.7",
    "@topcli/prompts": "^1.8.0",
    "@types/jest-image-snapshot": "^6",
    "@types/xmldom": "^0.1.34",
    "chromatic": "^7.6.0",
    "concurrently": "^8.2.2",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.6.15",
    "figma-api": "^1.11.0",
    "jest-image-snapshot": "^6.2.0",
    "lit-analyzer": "^2.0.2",
    "nodemon": "^3.0.2",
    "playwright": "^1.40.0",
    "postcss": "^8.4.32",
    "postcss-mixins": "^9.0.4",
    "postcss-nesting": "^12.0.1",
    "postcss-plugin": "^1.0.0",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.7",
    "tsx": "^3.14.0",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "xmldom": "^0.6.0"
  },
  "readme": "ERROR: No README data found!",
  "types": "./dist/index.d.ts",
  "customElements": "custom-elements.json",
  "files": [
    "dist/components/alert-button/alert-button.js"
  ]
}
